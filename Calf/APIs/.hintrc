{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-parent": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "color: var(--cucumber-anchor-color, oklch(62.3% 0.214 259.815deg))",
          "border: 1px solid var(--cucumber-panel-accent-color, oklch(92.9% 0.013 255.508deg))",
          "color: var(--cucumber-code-text-color, oklch(27.9% 0.041 260.031deg))",
          "color: var(--cucumber-keyword-color, oklch(37.9% 0.146 265.522deg))",
          "color: var(--cucumber-docstring-color, oklch(51.1% 0.096 186.391deg))",
          "color: var(--cucumber-parameter-color, oklch(62.3% 0.214 259.815deg))",
          "color: oklch(.723 .219 149.579/.75)"
        ]
      }
    ],
    "create-element-svg": "off",
    "meta-charset-utf-8": "off",
    "meta-viewport": "off"
  }
}